	Name = "Fast PL Farm (Requires Shadow)",
	Default = false,
	Callback = function(Value)
		ShadowTrainFarm = Value
	end    
})

Tab:AddSlider({
	Name = "Fast PL Settings",
	Min = 0,
	Max = 2,
	Default = 0,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.01,
	ValueName = "Seconds Between Hits",
	Callback = function(Value)
		ShadowTrainWait = Value
	end    
})


local maxDistance = 15
local KillAura = false
local firstTimeKillAura = false
Tab:AddToggle({
	Name = "Kill Aura (Low Chance Of Kick)",
	Default = false,
	Callback = function(Value)
		KillAura = Value
		
		if firstTimeKillAura == false and Value == true then
		firstTimeKillAura = true
		OrionLib:MakeNotification({
	Name = "Skrillex Hub",
	Content = "There is a little chance of you getting kicked",
	Image = "rbxassetid://4483345998",
	Time = 5
})
		
		end
	end    
})


Tab:AddSlider({
	Name = "Kill Aura Distance",
	Min = 5,
	Max = 15,
	Default = 10,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Meters",
	Callback = function(Value)
		maxDistance = Value
	end    
})

local PlayersKillAura = false
local firstTimeKillPlayers = false
Tab:AddToggle({
	Name = "Kill Aura Players",
	Default = false,
	Callback = function(Value)
		PlayersKillAura = Value
	if Value == true and firstTimeKillPlayers == false then
	    firstTimeKillPlayers = true
	OrionLib:MakeNotification({
	Name = "Skrillex Hub",
	Content = "High ban risk if caught",
	Image = "rbxassetid://4483345998",
	Time = 5

})
end
		
	end    
})

local keyPressSpeed = 0.1
Tab:AddSlider({
	Name = "Auto Gravity Chamber Speed",
	Min = 0.1,
	Max = 1,
	Default = 0.1,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.01,
